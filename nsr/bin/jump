#!/usr/bin/expect -f
# 
# two-factor-ssh.sh
# 
# Performs a two-factor SSH authentication.
# 
#================================================================================

set timeout 10
set log_user 0

spawn VIPAccess
expect -re ......

set secCode $expect_out(buffer)
spawn ssh $JUMPHOST
expect {
  "SecurityCode: " {
    send "$secCode\r" 
  }
} 
          
          
interact

